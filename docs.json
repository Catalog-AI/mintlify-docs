{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Catalog Documentation",
  "colors": {
    "primary": "#00B158",
    "light": "#00D166",
    "dark": "#008F47"
  },
  "favicon": "/logo/catalog-logo-black.svg",
  "navigation": {
    "anchors": [
      {
        "anchor": "Documentation",
        "icon": "book-open",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "welcome",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              "api-reference/overview",
              {
                "group": "Browse",
                "pages": [
                  "api-reference/endpoints/browse/list-vendors",
                  "api-reference/endpoints/browse/list-collections",
                  "api-reference/endpoints/browse/list-listings"
                ]
              },
              {
                "group": "Products",
                "pages": [
                  "api-reference/endpoints/products/get-product",
                  "api-reference/endpoints/products/get-products"
                ]
              },
              {
                "group": "Search",
                "pages": [
                  "api-reference/endpoints/search/agentic-search",
                  "api-reference/endpoints/search/agentic-search-mini",
                  "api-reference/endpoints/search/regular-search"
                ]
              },
              "api-reference/endpoints/affiliate/generate-links",
              "api-reference/endpoints/usage/get-usage"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/pagination",
              "guides/product-index",
              "guides/filtering"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "resources/error-codes",
              "resources/rate-limits"
            ]
          }
        ]
      },
      {
        "anchor": "Website",
        "href": "https://www.getcatalog.ai",
        "icon": "globe"
      },
      {
        "anchor": "Support",
        "href": "mailto:founders@getcatalog.ai",
        "icon": "envelope"
      }
    ]
  },
  "logo": {
    "light": "/logo/catalog-logo-black.svg",
    "dark": "/logo/catalog-logo-white.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "mailto:founders@getcatalog.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "seo": {
    "indexing": "all",
    "metatags": {
      "og:type": "website",
      "og:site_name": "Catalog API Documentation",
      "og:description": "Modern product discovery and intelligence API for agentic commerce applications",
      "twitter:card": "summary_large_image",
      "twitter:site": "@getcatalogai"
    }
  }
}
